<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" style="-fx-background-color: #2a2f72;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.SecondaryController">
    <children>
      <AnchorPane prefHeight="500.0" prefWidth="700.0">
         <children>
              <Label layoutX="287.0" layoutY="94.0" text="Gender:" textFill="#fcf8f8">
                  <font>
                      <Font name="Georgia" size="24.0" />
                  </font>
              </Label>
              <Button layoutX="268.0" layoutY="154.0" onAction="#handleMaleButtonClick" prefHeight="38.0" prefWidth="124.0" style="-fx-background-color: #00a79d;" text="Male" textFill="#fcf8f8" />
              <Button layoutX="268.0" layoutY="203.0" onAction="#handleFemaleButtonClick" prefHeight="38.0" prefWidth="124.0" style="-fx-background-color: #00a79d;" text="Female" textFill="#fcf8f8" />
              <Button layoutX="268.0" layoutY="253.0" onAction="#handleNeutralButtonClick" prefHeight="38.0" prefWidth="124.0" style="-fx-background-color: #00a79d;" text="Neutral" textFill="#fcf8f8" />
            <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToPrimary" text="&lt;" />
         </children>
      </AnchorPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
