<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" style="-fx-background-color: #2a2f72;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.PrimaryController">
   <children>
      <Label alignment="TOP_CENTER" text="Welcome To Lesson Plan!" textFill="#f5eeee">
         <font>
            <Font name="Georgia" size="44.0" />
         </font>
      </Label>
      <Label />
      <Button fx:id="CreatePlanButton" onAction="#switchToSecondary" prefHeight="38.0" prefWidth="124.0" style="-fx-background-color: #00a79d;" text="Create Plans" textFill="#fcf8f8" />
      <Button fx:id="BrowsePlanButton" onAction="#switchToBrowsePage" prefHeight="41.0" prefWidth="125.0" style="-fx-background-color: #00a79d;" text="Browse Plans" textFill="#fcf8f8" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
